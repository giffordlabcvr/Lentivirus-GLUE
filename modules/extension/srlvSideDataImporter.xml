<textFilePopulator>

	<columnDelimiterRegex>,</columnDelimiterRegex>
	<textFileColumn>
		<identifier>true</identifier>
		<header>Accession</header>
		<property>sequenceID</property>
	</textFileColumn>
	
	<textFileColumn>
		<header>Species</header>
		<property>isolate_data.host_sci_name</property>
		<overwriteExistingNonNull>true</overwriteExistingNonNull>		
	</textFileColumn>
	
	<textFileColumn>
		<header>Country</header>
		<property>isolate_data.country</property>
		<overwriteExistingNonNull>true</overwriteExistingNonNull>
	</textFileColumn>
	
	<textFileColumn>
		<header>Isolation Year</header>
		<property>isolate_data.collection_year</property>
		<nullRegex>NK</nullRegex>
		<overwriteExistingNonNull>true</overwriteExistingNonNull> <!-- will not update to NULL --> 		
	</textFileColumn>
	
	<textFileColumn>
		<header>Genotype</header>
		<property>clade</property>
		<overwriteExistingNonNull>true</overwriteExistingNonNull>
	</textFileColumn>
	
	<textFileColumn>
		<header>Subtype</header>
		<property>subtype</property>
		<overwriteExistingNonNull>true</overwriteExistingNonNull>
	</textFileColumn>
	
	<textFileColumn>
		<header>Strain/clone</header>
		<property>isolate_data.isolate</property>
		<overwriteExistingNonNull>true</overwriteExistingNonNull>
	</textFileColumn>
	
</textFilePopulator>

