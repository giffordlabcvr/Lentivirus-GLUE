<textFilePopulator>

	<columnDelimiterRegex>,</columnDelimiterRegex>
	<textFileColumn>
		<identifier>true</identifier>
		<header>Accession</header>
		<property>sequenceID</property>
	</textFileColumn>
	<textFileColumn>
		<header>Species</header>
		<property>host</property>
		<overwriteExistingNonNull>true</overwriteExistingNonNull>		
	</textFileColumn>
	<textFileColumn>
		<header>Country</header>
		<property>country</property>
		<overwriteExistingNonNull>true</overwriteExistingNonNull>
	</textFileColumn>
	<textFileColumn>
		<header>Isolation Year</header>
		<property>collection_year</property>
		<nullRegex>NK</nullRegex>
		<overwriteExistingNonNull>true</overwriteExistingNonNull> <!-- will not update to NULL --> 		
	</textFileColumn>
	<textFileColumn>
		<header>Genotype</header>
		<property>genotype</property>
		<overwriteExistingNonNull>true</overwriteExistingNonNull>
	</textFileColumn>
	<textFileColumn>
		<header>Subtype</header>
		<property>subtype</property>
		<overwriteExistingNonNull>true</overwriteExistingNonNull>
	</textFileColumn>
	<textFileColumn>
		<header>Strain/clone</header>
		<property>isolate</property>
		<overwriteExistingNonNull>true</overwriteExistingNonNull>
	</textFileColumn>
	<textFileColumn>
		<header>Region</header>
		<property>geo_region</property>
		<overwriteExistingNonNull>true</overwriteExistingNonNull>
	</textFileColumn>
	<textFileColumn>
		<header>Genomic region</header>
		<property>genome_region</property>
		<overwriteExistingNonNull>true</overwriteExistingNonNull>
	</textFileColumn>
	
</textFilePopulator>

