<maxLikelihoodGenotyper>
	<maxLikelihoodPlacerModuleName>srlvMaxLikelihoodPlacer</maxLikelihoodPlacerModuleName>
	<cladeCategory>
		<name>genotype</name>	
		<displayName>Genotype</displayName>	
		<whereClause>clade_category = 'genotype'</whereClause>
		<distanceScalingExponent>-4.5</distanceScalingExponent>
		<distanceCutoff>0.6</distanceCutoff>
		<finalCladeCutoff>75.0</finalCladeCutoff>
	</cladeCategory>
	<cladeCategory>
		<name>subtype</name>	
		<displayName>Subtype</displayName>	
		<whereClause>clade_category = 'subtype'</whereClause>
		<distanceScalingExponent>-4.5</distanceScalingExponent>
	    <distanceCutoff>0.45</distanceCutoff>
		<finalCladeCutoff>61.0</finalCladeCutoff>
	</cladeCategory>
</maxLikelihoodGenotyper>