
  module hivRaxmlPhylogenyGenerator    
    generate nucleotide phylogeny AL_UNCONSTRAINED_genome_REF --allMembers --outputFile trees/HIV-1_AL_UNCONSTRAINED_genome_REF.tre NEWICK_BOOTSTRAPS   
    exit    

  module hivPhyloUtility
    reroot-phylogeny -i trees/HIV-1_AL_UNCONSTRAINED_genome_REF.tre NEWICK_BOOTSTRAPS -m -o trees/HIV-1_AL_UNCONSTRAINED_genome_REF_midpoint.tre NEWICK_BOOTSTRAPS
    exit
    