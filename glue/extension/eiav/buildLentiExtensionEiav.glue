  project lentivirus

    # Delete previous extension build
    #run file glue/extension/eiav/lentiDeleteExtensionBuildEiav.glue

    # Import sources
    run file glue/extension/eiav/lentiExtensionSourcesEiav.glue

    # Define extension references
    run file glue/extension/eiav/lentiExtensionReferencesEiav.glue

    # Define feature locations for genus references
    run file glue/extension/eiav/lentiExtensionFeatureLocationsEiav.glue

	# Extract data from the XML files
	module lentiGenbankXmlPopulator populate

	# Link curated set sequences to isolate data
	run script glue/extension/eiav/lentiLoadIsolateDataEiav.js
  
	# Import metadata for exogenous reference sequences from tabular
	create module -f modules/extension/eiav/lentiSideDataImporterEiav.xml eiavSideDataImporter
	module eiavSideDataImporter 
	  populate -f tabular/extension/eiav-curated.csv
      exit

	# Add curated sequences constrained alignment tree
	run file glue/extension/eiav/lentiAlignmentsEiav.glue

	# Add curated sequences constrained alignment tree
	run file glue/extension/eiav/lentiAlignmentTreeEiav.glue
	
	# Record feature presence
	run file glue/extension/eiav/lentiRecordFeaturePresenceEiav.glue

	# Create phylogenies
	run file glue/extension/eiav/lentiCreatePhylogeniesEiav.glue

	# Validate
	validate

	exit
