alignment AL_ROOT
 
  add member -w "source.name like 'ncbi-curated-%'"
  demote member AL_FIV  -w "sequence.source.name = 'ncbi-curated-fiv'"
  demote member AL_SRLV -w "sequence.source.name = 'ncbi-curated-srlv'"
  demote member AL_BIV  -w "sequence.source.name = 'ncbi-curated-biv'"
  demote member AL_EIAV -w "sequence.source.name = 'ncbi-curated-eiav'"

  exit

