  project lentivirus

    # Delete previous extension build
    #run file glue/extension/biv/deleteExtensionBuildBiv.glue

    # Import sources
     import source sources/ncbi-nuccore-biv

	# Link nuccore sequences to isolate data table
	run script glue/extension/biv/linkIsolateNuccoreSeqsBiv.js
 

	# Extract data from the XML files
	create module -f modules/extension/biv/genbankXmlPopulatorBiv.xml
	module genbankXmlPopulatorBiv populate -w "source.name = 'ncbi-nuccore-biv'"

 
 
	exit

