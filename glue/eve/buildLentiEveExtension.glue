schema-project lentivirus
  run file glue/eve/lentiEveSchemaExtensions.glue
  exit

project lentivirus

  # Import NCBI reference sequences 
  import source sources/fasta-digs-eve

  # Link references to isolate data
  run script glue/eve/lentiEveLoadLocusData1.js
  run script glue/eve/lentiEveLoadLocusData2.js

  # Validate
  validate

  exit
